apiVersion: v1
kind: Service
metadata:
  name: "read-{{ .Service.Name.DNSCase }}"
  namespace: "{{ .Namespace }}"
spec:
  selector:
    app: "{{ .Service.Name.DNSCase}}-{{.Parameters.Parameters.ReadSelector}}"
  ports:
    - protocol: TCP
      name: tcp-port
      port: 6379
      targetPort: 6379
